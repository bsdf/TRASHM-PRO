KPRINTLNX: MACRO
	movem.l	d0-a6,-(sp)
	move.l	#.a\2,a0
	move.l	#0,a1
	jsr	KPrintF
	bra.s	.b\2
.a\2:	dc.b	\1
	dc.b	$a,0
	even
.b\2:	movem.l	(sp)+,d0-a6
	ENDM

